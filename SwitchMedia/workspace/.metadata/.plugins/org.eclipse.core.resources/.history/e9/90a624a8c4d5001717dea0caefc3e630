package com.codebind;

import org.openqa.selenium.By;

public class GmailInboxPage {

	public static By composeButton = By.cssSelector(".T-I.J-J5-Ji.T-I-KE.L3");
	public static By maximizeButton = By.xpath("//td//img[2]");
	public static By toField = By.xpath(".//textarea[contains(@aria-label, 'To')]");
	public static By subjectField = By.name("subjectbox");
	public static By emailBody = By.xpath("(.//*[@aria-label='Message Body'])[2]");
	public static By attachButton = By.xpath("//div[@class='a1 aaA aMZ']");
	public static By sendButton = By.xpath("//div[text()='Send']");
	public static By profilePic = By.xpath(".//*[@id='gb']/div[1]/div[1]/div[2]/div[5]/div[1]/a/span");
	public static By signOut = By.xpath(".//*[@id='gb_71']");
	
}
